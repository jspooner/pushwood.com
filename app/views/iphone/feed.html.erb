<% @images.each do |image| %>
<div class="feedImageHolder">
  <div class="userInfo">
    <div class="userName"><%= image.user.try(:full_name) %></div>
    <div class="uploadTime"><%= time_ago_in_words image.updated_at %></div>
  </div>
  <%= image.location.name %><br>

  <%= image_tag(image.img.url(:iosThumbnail)) %>
</div>
<% end %>
